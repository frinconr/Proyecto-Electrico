<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@drawable/background1"
    tools:context="com.example.felipe.harmony3.PhoneActivity"
    android:id="@+id/PhoneLinearLayout">
    <LinearLayout
        android:id="@+id/a"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/first_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="1"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
        <Button
            android:id="@+id/sixth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="6"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/b"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/second_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="2"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
        <Button
            android:id="@+id/seventh_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="7"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/c"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/third_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="3"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
        <Button
            android:id="@+id/eighth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="8"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/d"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/fourth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="4"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
        <Button
            android:id="@+id/nineth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="9"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/e"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal">
        <Button
            android:id="@+id/fifth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="5"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
        <Button
            android:id="@+id/tenth_button"
            android:layout_height="match_parent"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="10"
            android:textColor="@android:color/background_light"
            android:textSize="@dimen/abc_action_button_min_width_material"/>
    </LinearLayout>
</LinearLayout>